              <source srcset="{{ baumschule.image_url }}?nf_resize=fit&w=1024" media="(min-width: 769px)">
              <source srcset="{{ baumschule.image_url }}?nf_resize=fit&w=768" media="(min-width: 481px)">

              <img src="{{ baumschule.image_url }}?nf_resize=fit&w=480" alt="Baumschule in Bildern – Zimmern Garten- und Landschaftsbau GmbH" />
            </picture>
            
          </a>
        </li>
        {%- endfor -%}

        <!-- Baufelderschliessung -->
