        </li>
        {%- endfor -%}
        
      </ul>

      <!-- <button id="loadmore" class="btn btn_red" onclick="" type="button" data="fade-up">Mehr anzeigen</button> -->

    </div><!-- .sctn_content -->

  </div><!-- .wrapper -->
