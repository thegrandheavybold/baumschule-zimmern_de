              <img src="{{ baufelderschliessung.image_url }}?nf_resize=fit&w=480" alt="Baufelderschliessung in Bildern – Zimmern Garten- und Landschaftsbau GmbH" />
            </picture>
            
          </a>
        </li>
        {%- endfor -%}

        <!-- Idee, Beratung, Planung -->
        {%- for ideeberatungplanung in ideeberatungplanung_gallery.images -%}
        <li class="mix ideeberatungplanung">
          <a href="{{ ideeberatungplanung.image_url }}" class="gllry__zoom" data-dimbox="gllry_projekte" data-dimbox-caption="">
                
            <picture>
              <source srcset="{{ ideeberatungplanung.image_url }}" media="(min-width: 1025px)">
              <source srcset="{{ ideeberatungplanung.image_url }}?nf_resize=fit&w=1024" media="(min-width: 769px)">
              <source srcset="{{ ideeberatungplanung.image_url }}?nf_resize=fit&w=768" media="(min-width: 481px)">

              <img src="{{ ideeberatungplanung.image_url }}?nf_resize=fit&w=480" alt="Idee, Beratung, Planung in Bildern – Zimmern Garten- und Landschaftsbau GmbH" />
            </picture>
            
